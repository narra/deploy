# Intance
NARRA_STACK_NAME=narra
NARRA_INSTANCE_NAME=instance

# Ruby Environment
PASSENGER_APP_ENV=production
RUBY_VERSION=2.6.8
SECRET_KEY_BASE=replace_with_secure_key

# Hostnames
NARRA_API_HOSTNAME=api.narra
NARRA_STORAGE_HOSTNAME=storage.narra
NARRA_EDITOR_HOSTNAME=editor.narra
NARRA_ROUTER_HOSTNAME=router.narra

# Protocols
NARRA_API_PROTOCOL=http

# ADDITIONAL NARRA PLUGINS
# plugin_01*plugin_01_git_url;plugin_02*plugin_02_git_url
NARRA_PLUGINS=

# STORAGES
# MinIO specific variables
MINIO_ACCESS_KEY=minio
MINIO_SECRET_KEY=minio123

